							<OnLoad>
								VUHDO_lnfSetModel(self, "VUHDO_CONFIG.SHOW_INCOMING");
								VUHDO_lnfSetTooltip(self, VUHDO_I18N_TT_216);
							</OnLoad>
						</Scripts>
					</CheckButton>

					<CheckButton name="$parentLifeTextCheckButton" text="VUHDO_I18N_LIFE_TEXT" inherits="VuhDoCheckButtonTemplate">
