							<AbsDimension x="100" y="18" />
						</Size>
						<Anchors>
							<Anchor point="TOPLEFT">
								<Offset>
									<AbsDimension x="13" y="-7" />
								</Offset>
							</Anchor>
						</Anchors>
						<Layers>
							<Layer>
								<FontString name="$parentLabel" text="VUHDO_I18N_SHOW_INC" inherits="VuDoDefaultTitleFontTemplate">
